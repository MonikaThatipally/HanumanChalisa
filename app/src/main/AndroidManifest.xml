<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thatipally.monikathatipally.hanumanchalisa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.thatipally.monikathatipally.hanumanchalisa.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.thatipally.monikathatipally.hanumanchalisa.Telugu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thatipally.monikathatipally.hanumanchalisa.MainActivity" />
        </activity>
        <activity android:name="com.thatipally.monikathatipally.hanumanchalisa.Hindi">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thatipally.monikathatipally.hanumanchalisa.MainActivity" />
        </activity>
    </application>

</manifest>